{
  "name": "otp-js",
  "version": "0.0.1",
  "description": "open telecommunication protocol implementation for nodejs",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "pretest": "rimraf coverage",
    "test": "node --trace-warnings node_modules/.bin/jest --detectOpenHandles",
    "test:perf": "node --max-old-space-size=6144 --expose-gc --trace-warnings node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles -c ./jest.perf.config.ts",
    "watch": "node --trace-warnings node_modules/.bin/jest --detectOpenHandles --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndY0/otp-js.git"
  },
  "keywords": [
    "otp",
    "node",
    "scalable",
    "ipc",
    "redis",
    "yield",
    "style",
    "async",
    "serveur",
    "client"
  ],
  "author": "ndY0",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ndY0/otp-js/issues"
  },
  "homepage": "https://github.com/ndY0/otp-js#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/reflect-metadata": "^0.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "remove": "^0.1.5",
    "uuid": "^8.3.2"
  }
}
